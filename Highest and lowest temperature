#include <stdio.h>

int main() {
    int temp[7], i, high, low;

    for (i = 0; i < 7; i++) {
        printf("Enter temperature of day %d: ", i+1);
        scanf("%d", &temp[i]);
    }

    high = low = temp[0];
    for (i = 1; i < 7; i++) {
        if (temp[i] > high) high = temp[i];
        if (temp[i] < low) low = temp[i];
    }

    printf("Highest = %d\nLowest = %d", high, low);
    return 0;
}
