#include <stdio.h>

int main() {
    int marks[5], i, max, topper;

    for (i = 0; i < 5; i++)
        scanf("%d", &marks[i]);

    max = marks[0];
    topper = 1;

    for (i = 1; i < 5; i++) {
        if (marks[i] > max) {
            max = marks[i];
            topper = i + 1;
        }
    }

    printf("Topper is student %d with %d marks", topper, max);
    return 0;
}
