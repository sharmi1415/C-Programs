#include <stdio.h>

int main() {
    int a[5], b[5], i;

    printf("Enter 5 elements:\n");
    for(i = 0; i < 5; i++) {
        scanf("%d", &a[i]);
        b[i] = a[i];
    }

    printf("Original Array:\n");
    for(i = 0; i < 5; i++)
        printf("%d ", a[i]);

    printf("\nCopied Array:\n");
    for(i = 0; i < 5; i++)
        printf("%d ", b[i]);

    return 0;
}
