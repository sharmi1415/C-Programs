#include <stdio.h>
#include <string.h>

int main() {
    char username[20] = "admin";
    char password[20] = "1234";
    char u[20], p[20];

    printf("Enter Username: ");
    scanf("%s", u);
    printf("Enter Password: ");
    scanf("%s", p);

    if(strcmp(username, u) == 0 && strcmp(password, p) == 0)
        printf("Login Successful\n");
    else
        printf("Invalid Login\n");

    return 0;
}
