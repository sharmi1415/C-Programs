#include <stdio.h>

int main() {
    int n, i;
    int marks[50];
    int total = 0;

    printf("Enter number of students: ");
    scanf("%d", &n);

    printf("Enter marks of %d students:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &marks[i]);
        total = total + marks[i];
    }

    printf("Total marks of %d students = %d\n", n, total);

    return 0;
}
