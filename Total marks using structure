#include <stdio.h>

struct Student {
    int marks1, marks2, marks3;
};

int main() {
    struct Student s;
    int total;

    printf("Enter marks in Subject 1: ");
    scanf("%d", &s.marks1);

    printf("Enter marks in Subject 2: ");
    scanf("%d", &s.marks2);

    printf("Enter marks in Subject 3: ");
    scanf("%d", &s.marks3);

    total = s.marks1 + s.marks2 + s.marks3;

    printf("\nTotal Marks = %d\n", total);

    return 0;
}
